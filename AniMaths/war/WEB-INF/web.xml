<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>AniMaths.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>service</servlet-name>
    <servlet-class>fr.upmf.animaths.server.AniMathsServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>service</servlet-name>
    <url-pattern>/AniMaths/exercice</url-pattern>
  </servlet-mapping>


</web-app>
