<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<icon>
		<small-icon>favicon.png</small-icon>
	</icon>
	<welcome-file-list>
		<welcome-file>AniMaths.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>AniMathsServiceImpl</servlet-name>
		<servlet-class>fr.upmf.animaths.server.AniMathsServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AniMathsServiceImpl</servlet-name>
		<url-pattern>/animaths/service</url-pattern>
	</servlet-mapping>

</web-app>
